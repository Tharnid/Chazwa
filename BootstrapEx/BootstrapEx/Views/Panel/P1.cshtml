@{
    ViewBag.Title = "Panel Sample 1";
}

<link href="~/Content/pdsa-panel-additions.css" rel="stylesheet" />

<div class="panel panel-primary">
    <div class="panel-heading">
        <h1 class="panel-title">
            <span class="glyphicon glyphicon-music"></span>&nbsp;CountryMusic
            <a href="#" class="panel-right glyphicon glyphicon-random">&nbsp;Reload</a>
        </h1>
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-bordered table-striped">
